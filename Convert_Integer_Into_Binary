package mayank.Array;

public class ConvertIntegerIntoBinary {

	 public static void main(String[] args) 
	    {
	        int n=10;
	        String x = "";
	        while(n > 0)
	        {
	            int a = n % 2;
	            x = a + x;
	            n = n / 2;
	        }
	        System.out.println(x);
	    }
}
